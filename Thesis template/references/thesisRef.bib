@article{6965597,
  author   = {J. Luo and J. Hu and D. Wu and R. Li},
  title    = {Opportunistic Routing Algorithm for Relay Node Selection in Wireless Sensor Networks},
  journal  = {IEEE Transactions on Industrial Informatics},
  year     = {2015},
  volume   = {11},
  pages    = {112-121},
  number   = {1},
  month    = {Feb},
  abstract = {Energy savings optimization becomes one of the major concerns in the
	wireless sensor network (WSN) routing protocol design, due to the
	fact that most sensor nodes are equipped with the limited nonrechargeable
	battery power. In this paper, we focus on minimizing energy consumption
	and maximizing network lifetime for data relay in one-dimensional
	(1-D) queue network. Following the principle of opportunistic routing
	theory, multihop relay decision to optimize the network energy efficiency
	is made based on the differences among sensor nodes, in terms of
	both their distance to sink and the residual energy of each other.
	Specifically, an Energy Saving via Opportunistic Routing (ENS_OR)
	algorithm is designed to ensure minimum power cost during data relay
	and protect the nodes with relatively low residual energy. Extensive
	simulations and real testbed results show that the proposed solution
	ENS_OR can significantly improve the network performance on energy
	saving and wireless connectivity in comparison with other existing
	WSN routing schemes.},
  doi      = {10.1109/TII.2014.2374071},
  issn     = {1551-3203},
  keywords = {data communication;energy conservation;energy consumption;queueing
	theory;telecommunication network routing;wireless sensor networks;ENS_OR
	algorithm;WSN routing;energy consumption;energy saving via opportunistic
	routing;energy savings optimization;network energy efficiency;nonrechargeable
	battery power;one-dimensional queue network;opportunistic routing
	algorithm;relay node selection;sensor nodes;wireless sensor networks;Energy
	consumption;Informatics;Relays;Robot sensing systems;Routing;Wireless
	communication;Wireless sensor networks;Energy efficiency;One-dimensional
	queue network;WSN;energy efficiency;one-dimensional (1-D) queue network;opportunistic
	routing;relay node;wireless sensor network (WSN)}
}

