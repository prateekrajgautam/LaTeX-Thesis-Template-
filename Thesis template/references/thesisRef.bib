@article{8263170,
  author   = {H. Zhang and Y. Liang and W. Zhang and N. Xu and Z. Guo and G. Wu},
  title    = {Improved {PSO-}based Method for Leak Detection and Localization in Liquid Pipelines},
  journal  = {IEEE Transactions on Industrial Informatics},
  year     = {2018},
  volume   = {PP},
  pages    = {1-1},
  number   = {99},
  abstract = {Based on inverse hydraulic thermodynamic transient analyses and on
	an improved particle swarm optimization (PSO), a leak detection and
	localization method is proposed for liquid pipelines. The finite
	volume method, was employed to numerically model the continuity,
	momentum, and energy equations. To determine the optimum improved
	PSO algorithm, and corresponding parameters, four types of algorithms
	were used for the analyses of a virtual pipeline. Accuracy, stability,
	robustness, and false alarm rate tests were conducted. The SIPSO
	algorithm has been shown to outperform the other algorithms. Two
	oil pipelines were utilized as example cases in conjunction with
	the use of the SIPSO algorithm. One of these pipelines was involved
	in a field opening experiment, while the other was involved in a
	real leak accident. The results demonstrated that smaller relative
	errors were achieved with the proposed method for the estimations
	of the location, coefficient, and starting time of the leak.},
  doi      = {10.1109/TII.2018.2794987},
  issn     = {1551-3203},
  keywords = {Algorithm design and analysis;Correlation;Leak detection;Mathematical
	model;Pipelines;Thermodynamics;Transient analysis;Improved particle
	swarm optimization;inverse hydraulic-hermodynamic transient analysis;leak
	detection and location;liquid pipelines}
}

