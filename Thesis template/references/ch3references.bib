@article{6965597,
  author   = {J. Luo and J. Hu and D. Wu and R. Li},
  journal  = {IEEE Transactions on Industrial Informatics},
  title    = {Opportunistic Routing Algorithm for Relay Node Selection in Wireless Sensor Networks},
  year     = {2015},
  volume   = {11},
  number   = {1},
  pages    = {112-121},
  abstract = {Energy savings optimization becomes one of the major concerns in the wireless sensor network (WSN) routing protocol design, due to the fact that most sensor nodes are equipped with the limited nonrechargeable battery power. In this paper, we focus on minimizing energy consumption and maximizing network lifetime for data relay in one-dimensional (1-D) queue network. Following the principle of opportunistic routing theory, multihop relay decision to optimize the network energy efficiency is made based on the differences among sensor nodes, in terms of both their distance to sink and the residual energy of each other. Specifically, an Energy Saving via Opportunistic Routing (ENS_OR) algorithm is designed to ensure minimum power cost during data relay and protect the nodes with relatively low residual energy. Extensive simulations and real testbed results show that the proposed solution ENS_OR can significantly improve the network performance on energy saving and wireless connectivity in comparison with other existing WSN routing schemes.},
  keywords = {data communication;energy conservation;energy consumption;queueing theory;telecommunication network routing;wireless sensor networks;ENS_OR algorithm;WSN routing;energy consumption;energy saving via opportunistic routing;energy savings optimization;network energy efficiency;nonrechargeable battery power;one-dimensional queue network;opportunistic routing algorithm;relay node selection;sensor nodes;wireless sensor networks;Energy consumption;Informatics;Relays;Robot sensing systems;Routing;Wireless communication;Wireless sensor networks;Energy efficiency;One-dimensional queue network;WSN;energy efficiency;one-dimensional (1-D) queue network;opportunistic routing;relay node;wireless sensor network (WSN)},
  doi      = {10.1109/TII.2014.2374071},
  issn     = {1551-3203},
  month    = {Feb}
}

@article{7018937,
  author   = {K. Derr and M. Manic},
  journal  = {IEEE Transactions on Industrial Informatics},
  title    = {Wireless Sensor Networks$-$Node Localization for Various Industry Problems},
  year     = {2015},
  volume   = {11},
  number   = {3},
  pages    = {752-762},
  abstract = {Fast and effective monitoring following airborne releases of toxic substances is critical to mitigate risks to threatened population areas. Electrically powered systems in industrial settings require monitoring of emitted electromagnetic fields to determine the status of the equipment and ensure their safe operation. In situations such as these, wireless sensor nodes (WSNs) at fixed predetermined locations provide monitoring to ensure safety. A challenging algorithmic problem is determining the locations to place these WSNs while meeting several criteria: (1) to provide complete coverage of the domain; (2) to create a topology with problem-dependent node densities; and (3) to minimize the number of WSNs. This paper presents a novel approach, advancing front mesh generation with constrained Delaunay triangulation and smoothing (AFECETS) that addresses these criteria. A unique aspect of AFECETS is the ability to determine WSN locations for areas of high interest (hospitals, schools, and high population density areas) that require higher density of nodes for monitoring environmental conditions, a feature that is difficult to find in other research work. The AFECETS algorithm was tested on several arbitrary shaped domains. AFECETS simulation results show that the algorithm provides significant reduction in the number of nodes, in some cases over 40%, compared with an advancing front mesh generation algorithm; maintains and improves optimal spacing between nodes; and produces simulation run times suitable for real-time applications.},
  keywords = {electromagnetic fields;mesh generation;sensor placement;smoothing methods;telecommunication network topology;wireless sensor networks;AFECETS;WSN;advancing front mesh generation with constrained Delaunay triangulation and smoothing;emitted electromagnetic field monitoring;problem-dependent node densities;;risk mitigation;sensor node localization;wireless sensor network;Mesh generation;Monitoring;Robot sensing systems;Sociology;Statistics;Wireless sensor networks;Delaunay triangulation;Wireless sensor network;mesh generation;mesh network;sensor node;sensor node (SN);topology;wireless sensor network (WSN)},
  doi      = {10.1109/TII.2015.2396007},
  issn     = {1551-3203},
  month    = {June}
}


