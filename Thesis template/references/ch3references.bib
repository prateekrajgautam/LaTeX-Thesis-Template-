@article{7576704,
  author   = {Z. Iqbal and K. Kim and H. N. Lee},
  journal  = {IEEE Transactions on Industrial Informatics},
  title    = {A Cooperative Wireless Sensor Network for Indoor Industrial Monitoring},
  year     = {2017},
  volume   = {13},
  number   = {2},
  pages    = {482-491},
  abstract = {Industrial wireless sensor networks are getting popular for indoor monitoring of heavy machinery and large factories to make a reliable decision on the state of machines in a certain area of interest. However, the indoor wireless communication channel is not always reliable, and observations of some sensors cannot be reported successfully to the base station. In order to deal with this problem, we propose a cooperative WSN scheme by introducing a novel cooperation mechanism and a medium access control protocol. The proposed scheme effectively increases the probability of correct decision about the state of the machine, reduces the probability of false alarms at a given signal level, and reduces the overall energy consumption as compared to noncooperative schemes. We also present a closed-form expression for the symbol-error rate analysis of the proposed scheme, which shows that our proposed scheme achieves full diversity order offered by the cooperation scheme.},
  keywords = {condition monitoring;cooperative systems;production engineering computing;wireless sensor networks;cooperative WSN scheme;cooperative wireless sensor network;factories;false alarm probability;heavy machinery;indoor industrial monitoring;symbol error rate analysis;Base stations;Media Access Protocol;Monitoring;Relays;Sensors;Wireless communication;Wireless sensor networks;Cooperative communication;indoor industrial monitoring;industrial wireless sensor networks (IWSNs);machine condition monitoring (MCM);medium access control (MAC)},
  doi      = {10.1109/TII.2016.2613504},
  issn     = {1551-3203},
  month    = {April}
}



@article{7921558,
  author   = {P. Y. Chiang and P. C. P. Chao and D. C. Tarng and C. Y. Yang},
  journal  = {IEEE Transactions on Industrial Electronics},
  title    = {A Novel Wireless Photoplethysmography Blood-Flow Volume Sensor for Assessing Arteriovenous Fistula of Hemodialysis Patients},
  year     = {2017},
  volume   = {64},
  number   = {12},
  pages    = {9626-9635},
  abstract = {A novel blood-flow volume (BFV) sensor is presented for assessing quality of arteriovenous fistula in hemodialysis patients via noninvasive reflectance-type photoplethysmography (PPG). BFV is nowadays in clinic practices evaluated using an ultrasound Doppler monitor, which is expensive, bulky, and can only be operated by well-trained medical personnels. This study is devoted to develop a low-cost, small-sized, portable, and easy-to-use PPG sensor that is capable of continuous measurement of BFV. New designs of front-end analog circuits, signal processing, and an intelligent neural network calibration method are employed to finally achieve high correlation (R2 = 0.7176), as opposed to the ultrasound Doppler monitor, with the root-mean-squared errors successfully controlled under 289 ml/min.},
  keywords = {Doppler measurement;biomedical ultrasonics;blood;blood flow measurement;blood vessels;diseases;medical signal processing;neural nets;patient monitoring;photoplethysmography;PPG sensor;arteriovenous fistula;front-end analog circuits;hemodialysis patients;intelligent neural network calibration method;noninvasive reflectance-type photoplethysmography;root-mean-squared errors;signal processing;ultrasound Doppler monitor;wireless photoplethysmography blood-flow volume sensor;Biomedical monitoring;Blood;Blood flow;Doppler effect;Light emitting diodes;Monitoring;Ultrasonic imaging;Arteriovenous fistula (AVF);blood-flow volume (BFV);photoplethysmography (PPG) sensor},
  doi      = {10.1109/TIE.2017.2701780},
  issn     = {0278-0046},
  month    = {Dec}
}






@article{8063349,
  author   = {W. Cui and L. Zhang and B. Li and J. Guo and W. Meng and H. Wang and L. Xie},
  journal  = {IEEE Transactions on Industrial Informatics},
  title    = {Received-Signal-Strength based Indoor Positioning Using Random Vector Functional Link Network},
  year     = {2017},
  volume   = {PP},
  number   = {99},
  pages    = {1-1},
  abstract = {Fingerprinting based indoor positioning system (IPS) is gaining more research interest under the umbrella of location-based services. However, existing works have certain limitations in addressing issues such as noisy measurements, high computational complexity, poor generalization ability. In this work, a random vector functional link network based approach is introduced to address these issues. In the proposed system, a subset of informative features from many randomized noisy features are selected to both reduce the computational complexity and boost the generalization ability. Moreover, the feature selector and predictor are jointly learned iteratively in a single framework based on Augmented Lagrangian method. The proposed system is appealing as it can be naturally fit into parallel or distributed computing environment. Extensive realworld indoor localization experiments are conducted on users with smartphone devices and results demonstrate the superiority of the proposed method over the existing approaches.},
  keywords = {Databases;IP networks;Informatics;Mobile handsets;Noise measurement;Training;Wireless fidelity;Fingerprinting;Indoor Positioning System;Random Vector Functional Link Network;Wi-Fi},
  doi      = {10.1109/TII.2017.2760915},
  issn     = {1551-3203},
  month    = {}
}

@article{8263170,
  author   = {H. Zhang and Y. Liang and W. Zhang and N. Xu and Z. Guo and G. Wu},
  journal  = {IEEE Transactions on Industrial Informatics},
  title    = {Improved PSO-based Method for Leak Detection and Localization in Liquid Pipelines},
  year     = {2018},
  volume   = {PP},
  number   = {99},
  pages    = {1-1},
  abstract = {Based on inverse hydraulic thermodynamic transient analyses and on an improved particle swarm optimization (PSO), a leak detection and localization method is proposed for liquid pipelines. The finite volume method, was employed to numerically model the continuity, momentum, and energy equations. To determine the optimum improved PSO algorithm, and corresponding parameters, four types of algorithms were used for the analyses of a virtual pipeline. Accuracy, stability, robustness, and false alarm rate tests were conducted. The SIPSO algorithm has been shown to outperform the other algorithms. Two oil pipelines were utilized as example cases in conjunction with the use of the SIPSO algorithm. One of these pipelines was involved in a field opening experiment, while the other was involved in a real leak accident. The results demonstrated that smaller relative errors were achieved with the proposed method for the estimations of the location, coefficient, and starting time of the leak.},
  keywords = {Algorithm design and analysis;Correlation;Leak detection;Mathematical model;Pipelines;Thermodynamics;Transient analysis;Improved particle swarm optimization;inverse hydraulic-hermodynamic transient analysis;leak detection and location;liquid pipelines},
  doi      = {10.1109/TII.2018.2794987},
  issn     = {1551-3203},
  month    = {}
}



